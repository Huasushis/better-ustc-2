{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "better-ustc-2",
  "version": "0.1.0",
  "identifier": "com.cscourse.better-ustc-2",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "better-ustc-2",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/i32x32.png",
      "icons/i128x128.png",
      "icons/i256x256.png",
      "icons/i.icns",
      "icons/i.ico"
    ]
  }
}
